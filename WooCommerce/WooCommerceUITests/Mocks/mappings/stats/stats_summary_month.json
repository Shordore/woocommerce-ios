{
    "request": {
        "method": "GET",
        "urlPath": "/rest/v1.1/sites/161477129/stats/summary/",
        "queryParameters": {
            "period": {
                "equalTo": "month"
            },
            "date": {
                "matches": "(.*)"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "date": "{{now format='yyyy-MM-dd'}}",
            "period": "month",
            "views": 0,
            "visitors": 0,
            "likes": 0,
            "reblogs": 0,
            "comments": 0,
            "followers": 1
        }
    }
}
